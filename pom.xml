<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.musket.docker.manager</groupId>
  <artifactId>manger</artifactId>
  <packaging>pom</packaging>
  <version>1.0-SNAPSHOT</version>
    <modules>
        <module>docker.manager.site</module>
        <module>docker.manager.service</module>
        <module>docker.manager.engine</module>
        <module>docker.manager.handle</module>
    </modules>
    <name>manger Maven Webapp</name>
  <url>http://maven.apache.org</url>
    <!--添加版本号-->
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <springmvc.version>4.2.4.RELEASE</springmvc.version>
    </properties>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
